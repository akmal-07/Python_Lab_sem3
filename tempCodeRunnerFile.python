# Test case 5
tc5_input = "7\n11 9 8 6 7 6 1\n"
tc5_output = "1\n"

def write_file(filename, content):
    with open(filename, "w") as f:
        f.write(content)

write_file("input1.txt", tc1_input)
write_file("output1.txt", tc1_output)
write_file("input2.txt", tc2_input)
write_file("output2.txt", tc2_output)
write_file("input3.txt", tc3_input)
write_file("output3.txt", tc3_output)
write_file("input4.txt", tc4_input)
write_file("output4.txt", tc4_output)
write_file("input5.txt", tc5_input)
write_file("output5.txt", tc5_output)

print("Test case files generated successfully.")
