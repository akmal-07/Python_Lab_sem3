# Improved script for test cases

# Test case 1
tc1_input = "1\n42\n"
tc1_output = "0\n"

# Test case 2
n2 = 10000
numbers = ["100"] * n2
tc2_input = f"{n2}\n{' '.join(numbers)}\n"
tc2_output = f"{n2*(n2-1)//2}\n"

# Test case 3
tc3_input = "8\n3 1 2 3 2 1 3 1\n"
tc3_output = "7\n"

# Test case 4
tc4_input = "6\n4 5 6 7 8 9\n"
